app ShakenNotStirred

enum BeverageType {
    BEER,
    LIQUOR,
    WINE,
}

type User(name, joinedAt, email, phoneNumber) {
    // this is a description example
    name: Name
    joinedAt: DateTime = Now()
    email: ?Email
    phoneNumber: PhoneNumber

    @namespace(internal)
    canonicalID: CanonicalID
}

type Beverage {
    name: Name
    proof: Number
    type: BeverageType
}

@namespace(internal)
type Rating {
    user: User
    rating: Number
}